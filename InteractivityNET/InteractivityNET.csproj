<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>gspalato</Authors>
        <Description>An interactivity library for Discord.NET.</Description>
        <RepositoryUrl>https://github.com/gspalato/interactivity.net</RepositoryUrl>
        <Version>0.1.0-dev</Version>
        <PackageTags>Interactivity, Discord.NET, Discord</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Discord.Net" Version="2.2.0-dev-20200331.2" />
		<PackageReference Include="Discord.Net.Core" Version="2.2.0-dev-20200331.2" />
		<PackageReference Include="Discord.Net.WebSocket" Version="2.2.0-dev-20200331.2" />
	</ItemGroup>
</Project>